{
  "name": "@appliedblockchain/silentdatarollup-custom-rpc",
  "version": "1.0.1",
  "description": "Custom RPC for Silent Data [Rollup]",
  "author": "Applied Blockchain",
  "homepage": "https://github.com/appliedblockchain/silent-data-rollup-providers#readme",
  "keywords": [
    "ethereum",
    "provider",
    "silentdata",
    "rollup",
    "custom-rpc"
  ],
  "license": "MIT",
  "repository": "https://github.com/appliedblockchain/silent-data-rollup-providers",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": "dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --clean",
    "check-exports": "attw --pack . --profile node16",
    "dev": "concurrently --kill-others-on-fail -s all -p '[{name}]' -n 'HARDHAT,CUSTOM RPC' -c 'bgGreen.bold,bgBlue.bold' 'hardhat node' 'nodemon'",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@appliedblockchain/silentdatarollup-core": "1.0.1",
    "body-parser": "1.20.3",
    "dotenv": "16.4.7",
    "ethers": "6.13.2",
    "express": "4.21.2",
    "http-proxy-middleware": "3.0.5"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "22.5.4",
    "concurrently": "9.1.2",
    "hardhat": "^0.0.7",
    "nodemon": "3.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
