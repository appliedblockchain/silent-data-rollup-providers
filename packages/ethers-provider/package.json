{
  "name": "@appliedblockchain/silentdatarollup-ethers-provider",
  "version": "1.0.6",
  "description": "Ethers.js provider for Silent Data [Rollup]",
  "author": "Applied Blockchain",
  "homepage": "https://github.com/appliedblockchain/silent-data-rollup-providers#readme",
  "keywords": [
    "ethereum",
    "provider",
    "silentdata",
    "rollup",
    "ethers"
  ],
  "license": "MIT",
  "repository": "https://github.com/appliedblockchain/silent-data-rollup-providers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsup src/index.ts --format cjs,esm --dts --clean",
    "check-exports": "attw --pack . --profile node16",
    "prepack": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@appliedblockchain/silentdatarollup-core": "1.0.6",
    "debug": "4.3.7",
    "ethers": "6.13.2"
  },
  "devDependencies": {
    "@types/node": "22.5.4",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
