{
  "name": "@appliedblockchain/silentdatarollup-viem",
  "version": "1.0.11",
  "description": "Viem package for Silent Data",
  "author": "Applied Blockchain",
  "homepage": "https://github.com/appliedblockchain/silent-data-rollup-providers#readme",
  "keywords": [
    "ethereum",
    "provider",
    "silentdata",
    "rollup",
    "ethers",
    "viem"
  ],
  "license": "MIT",
  "repository": "https://github.com/appliedblockchain/silent-data-rollup-providers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsup src/index.ts --format cjs,esm --dts --clean",
    "check-exports": "attw --pack . --profile node16",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@appliedblockchain/silentdatarollup-ethers-provider": "workspace:*",
    "viem": "2.31.4"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
