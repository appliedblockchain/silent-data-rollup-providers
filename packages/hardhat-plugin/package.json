{
  "name": "@appliedblockchain/silentdatarollup-hardhat-plugin",
  "version": "1.0.7",
  "description": "Hardhat plugin for Silent Data [Rollup]",
  "author": "Applied Blockchain",
  "homepage": "https://github.com/appliedblockchain/silent-data-rollup-providers#readme",
  "keywords": [
    "ethereum",
    "provider",
    "silentdata",
    "rollup",
    "hardhat"
  ],
  "license": "MIT",
  "repository": "https://github.com/appliedblockchain/silent-data-rollup-providers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsup src/index.ts --format cjs,esm --dts --clean",
    "check-exports": "attw --pack . --profile node16",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@appliedblockchain/silentdatarollup-core": "1.0.7",
    "@nomicfoundation/hardhat-ethers": "3.0.8",
    "debug": "4.3.7",
    "ethers": "6.13.2"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "@types/node": "22.5.4",
    "hardhat": "2.22.10",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  },
  "peerDependencies": {
    "ethers": "^6.0.0",
    "hardhat": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
