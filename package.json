{
  "name": "@appliedblockchain/silentdatarollup-providers",
  "description": "Custom providers for Silent Data compatible with ethers.js, fireblocks and Hardhat plugins",
  "author": "@appliedblockchain",
  "homepage": "https://github.com/appliedblockchain/silent-data-rollup-providers#readme",
  "keywords": [
    "ethereum",
    "provider",
    "silentdata",
    "rollup",
    "ethers",
    "fireblocks",
    "hardhat",
    "custom-rpc",
    "viem"
  ],
  "license": "MIT",
  "repository": "https://github.com/appliedblockchain/silent-data-rollup-providers",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pnpm --color=always --filter \"./packages/*\" build",
    "changeset:publish": "changeset publish",
    "changeset:version": "changeset version",
    "changeset": "changeset",
    "check-exports": "pnpm --filter \"./packages/*\" check-exports",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "jest --projects packages/*"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@changesets/cli": "2.28.1",
    "@eslint/js": "9.22.0",
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.14",
    "@types/node": "24.10.1",
    "eslint": "9.31.0",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "29.2.5",
    "tsup": "8.4.0",
    "typescript": "5.6.2",
    "typescript-eslint": "8.26.1"
  }
}
